@prefix smarthome: <http://enilink.net/vocab/smarthome#> .
@prefix komma: <http://enilink.net/vocab/komma#> .
@prefix : <http://example.org/komma/smarthome-example.ttl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://example.org/komma/smarthome-example.ttl> a owl:Ontology ;
	owl:imports <http://enilink.net/vocab/smarthome> .

:1st_floor smarthome:contains :Corridor , :Kitchen , :Living_Room ;
	a smarthome:Floor ;
	rdfs:label "1st floor" .

:2nd_floor smarthome:contains :Bath , :Sleeping_Room ;
	a smarthome:Floor ;
	rdfs:label "2nd floor" .

:Bath a smarthome:Room .

:CentralSwitch smarthome:controls :Switch1 , :Switch2 ;
	smarthome:on true ;
	a smarthome:Switch .

:Corridor smarthome:contains :CentralSwitch ;
	a smarthome:Room .

:Kitchen smarthome:contains :Lamp1 , :Lamp2 , :Switch1 , :Switch2 ;
	a smarthome:Room .

:Lamp1 smarthome:on true ;
	a smarthome:Lamp .

:Lamp2 smarthome:on true ;
	a smarthome:Lamp .

:Living_Room a smarthome:Room ;
	rdfs:label "Living Room" .

:Sleeping_Room a smarthome:Room ;
	rdfs:label "Sleeping Room" .

:Switch1 smarthome:controls :Lamp1 , :Switch2 ;
	smarthome:on true ;
	a smarthome:Switch .

:Switch2 smarthome:controls :Lamp2 ;
	smarthome:on true ;
	a smarthome:Switch .

:Switch3 smarthome:controls :Lamp2 ;
	smarthome:on true ;
	a smarthome:Switch .

:myHouse smarthome:contains :1st_floor , :2nd_floor ;
	a smarthome:House ;
	rdfs:label "My House" .
